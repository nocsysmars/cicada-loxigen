    uint16_t  type == 0xffff;         // OFPT_EXPERIMENTER
    uint16_t  len;
    uint32_t  experimenter == 0x1018; // Experimenter ID:

    uint16_t  subtype == 1;          // OFDPA_ACT_PUSH_L2_Header =  1
    pad(6);
};

// Pop L2 Header
// Arguments: None
// Pop the outermost Ethernet header from the packet.
